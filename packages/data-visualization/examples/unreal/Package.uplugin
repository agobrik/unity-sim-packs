    "IsBetaVersion": false,
    "IsExperimentalVersion": false,
    "Installed": false,
    "RequiresBuildPlatform": false,

    "Modules": [
        {
            "Name": "DataVisualization",
            "Type": "Runtime",
            "LoadingPhase": "PostConfigInit",
            "AdditionalDependencies": [
                "Engine",
                "CoreUObject",
                "Core"
            ]
        },
        {
            "Name": "DataVisualizationEditor",
            "Type": "Editor",
            "LoadingPhase": "PostEngineInit",
            "AdditionalDependencies": [
                "DataVisualization",
                "UnrealEd",
                "ToolMenus",
                "EditorStyle",
                "EditorWidgets",
                "Slate",
                "SlateCore"
            ]
        }
    ],

    "Plugins": [
        {
            "Name": "V8JavaScript",
            "Enabled": true,
            "Optional": false,
            "Description": "Required for JavaScript engine integration"
        },
        {
            "Name": "HTTP",
            "Enabled": true,
            "Optional": false,
            "Description": "Required for web data streaming"
        },
        {
            "Name": "Json",
            "Enabled": true,
            "Optional": false,
            "Description": "Required for JSON data processing"
        }
    ],

    "LocalizationTargets": [
        {
            "Name": "DataVisualization",
            "LoadingPolicy": "Always"
        }
    ],

    "EnabledByDefault": false,
    "ExplicitlyLoaded": false,
    "HasExplicitPlatforms": false,
    "SupportedTargetPlatforms": [
        "Win64",
        "Mac",
        "Linux",
        "iOS",
        "Android"
    ],

    "SupportedPrograms": [
        "UnrealBuildTool",
        "UnrealHeaderTool"
    ],

    "PreBuildSteps": {
        "Win64": [
            "npm install --prefix $(ProjectDir)/Content/JavaScript @steamproject/data-visualization"
        ],
        "Mac": [
            "npm install --prefix $(ProjectDir)/Content/JavaScript @steamproject/data-visualization"
        ],
        "Linux": [
            "npm install --prefix $(ProjectDir)/Content/JavaScript @steamproject/data-visualization"
        ]
    },

    "PostBuildSteps": {
        "Win64": [],
        "Mac": [],
        "Linux": []
    },

    "InstallationInstructions": {
        "Steps": [
            "1. Ensure Node.js is installed on your system",
            "2. Install V8JavaScript plugin from Epic Games Marketplace",
            "3. Enable V8JavaScript plugin in Project Settings > Plugins",
            "4. Copy DataVisualization plugin to your project's Plugins folder",
            "5. Enable DataVisualization plugin in Project Settings > Plugins",
            "6. Run 'npm install @steamproject/data-visualization' in Content/JavaScript/",
            "7. Restart Unreal Editor",
            "8. Add DataVisualizationBridge component to your actors",
            "9. Initialize and start creating charts!"
        ],
        "Requirements": [
            "Unreal Engine 5.0 or later",
            "V8JavaScript plugin",
            "Node.js 16.0 or later",
            "C++ development environment",
            "Minimum 4GB RAM (8GB recommended)",
            "DirectX 11 or OpenGL 4.3 compatible graphics card"
        ]
    },

