{
    "FileVersion": 3,
    "Version": 1,
    "VersionName": "1.0.0",
    "FriendlyName": "SteamProject Data Visualization",
    "Description": "Real-time data visualization plugin for Unreal Engine game development. Provides charts, dashboards, and analytics visualization with JavaScript integration via V8 engine.",
    "Category": "Game Development Tools",
    "CreatedBy": "SteamProject Team",
    "CreatedByURL": "https://steamproject.com",
    "DocsURL": "https://docs.steamproject.com/data-visualization",
    "MarketplaceURL": "",
